Host linode
	Hostname 
	User root
	PubKeyAuthentication yes
	IdentityFile ~/.ssh/id_key
	IdentitiesOnly yes

Host win11
	Hostname localhost
	User 
	PubKeyAuthentication yes
	IdentityFile ~/.ssh/id_key
	IdentitiesOnly yes

Host ghosts
	Hostname localhost
	User ghost
	PubKeyAuthentication yes
	IdentityFile ~/.ssh/id_key
	IdentitiesOnly yes

Host cloud-class-aws
	Hostname 
	User admin
	PubKeyAuthentication yes
	IdentityFile ~/.ssh/id_cloud-class-aws
	IdentitiesOnly yes

Host gw.hpc 
	Hostname gw.hpc.nyu.edu
	User 


Host greene 
	ProxyJump gw.hpc 
	user
	PubKeyAuthentication yes
	PasswordAuthentication no
	IdentityFile ~/.ssh/id_key
	IdentitiesOnly yes
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null	
	RequestTTY yes
	RemoteCommand export PATH="$HOME/neovim/bin:$PATH" && bash -l

Host github
	Hostname github.com
	User git
	PubKeyAuthentication yes
	IdentityFile ~/.ssh/id_key
	IdentitiesOnly yes



