Host linode
	Hostname linode.ghost-cat.xyz
	User root
	PubKeyAuthentication yes
	IdentityFile ~/.ssh/id_key.secrets
	IdentitiesOnly yes

Host win11
	Hostname localhost
	User <no_user>
	PubKeyAuthentication yes
	IdentityFile ~/.ssh/id_key.secrets
	IdentitiesOnly yes

Host ghosts
	Hostname localhost
	User ghost
	PubKeyAuthentication yes
	IdentityFile ~/.ssh/id_key.secrets
	IdentitiesOnly yes

Host gw.hpc 
	Hostname gw.hpc.nyu.edu
	User ssh_nyu_user

Host greene 
	ProxyJump gw.hpc 
	PubKeyAuthentication yes
	PasswordAuthentication no
	IdentityFile ~/.ssh/id_key.secrets
	IdentitiesOnly yes
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null	
	RequestTTY yes
	RemoteCommand export PATH="$HOME/neovim/bin:$PATH" && bash -l
	ServerAliveInterval 60
	User ssh_nyu_user

Host github
	Hostname github.com
	User git
	PubKeyAuthentication yes
	IdentityFile ~/.ssh/id_key.secrets
	IdentitiesOnly yes



